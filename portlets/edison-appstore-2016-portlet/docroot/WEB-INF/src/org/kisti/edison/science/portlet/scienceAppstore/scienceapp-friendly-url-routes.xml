<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" 
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
		<pattern>/{appname}/{appversion}/view</pattern>
		<generated-parameter name="name">{appname}</generated-parameter>
		<generated-parameter name="version">{appversion}</generated-parameter>
		<generated-parameter name="myRender">friendlyAppRender</generated-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
</routes>
