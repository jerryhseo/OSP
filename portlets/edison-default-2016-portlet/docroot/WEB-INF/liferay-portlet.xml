<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>edison-privateinfoporlicy</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>privateinfopolicy</friendly-url-mapping>
		<friendly-url-routes>org/kisti/edison/privateinfopolicy/friendly-url-router.xml</friendly-url-routes>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>content-sub</css-class-wrapper>		
		<add-default-resource>true</add-default-resource>		
	</portlet>
	
	<portlet>
		<portlet-name>edison-termsofuse</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>termsofuse</friendly-url-mapping>
		<friendly-url-routes>org/kisti/edison/termsofuse/friendly-url-router.xml</friendly-url-routes>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>content-sub</css-class-wrapper>
		<add-default-resource>true</add-default-resource>		
	</portlet>
	
	<portlet>
		<portlet-name>edison-orgcodesearch</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>orgcodesearch</friendly-url-mapping>
		<friendly-url-routes>org/kisti/edison/orgcodesearch/friendly-url-router.xml</friendly-url-routes>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>content-sub</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	
	
	<portlet>
		<portlet-name>edison-portalagree</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>portalagree</friendly-url-mapping>
		<friendly-url-routes>org/kisti/edison/portalagree/friendly-url-router.xml</friendly-url-routes>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>content-sub</css-class-wrapper>
		<add-default-resource>true</add-default-resource>		
	</portlet>
		
	<portlet>
		<portlet-name>edison-site-join</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
		<css-class-wrapper>edison-site-join</css-class-wrapper>
	</portlet>	
	
	<portlet>
		<portlet-name>edison-relate-asset</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.kisti.edison.asset.portlet.ConfigurationController</configuration-action-class>
		<instanceable>true</instanceable>
		<css-class-wrapper>edison-relate-asset</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	
	<portlet>
		<portlet-name>edison-my-page</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.kisti.edison.mypage.ConfigurationController</configuration-action-class>
		<instanceable>false</instanceable>
		<css-class-wrapper>edison-my-page container</css-class-wrapper>
	</portlet>	
	
	
	<portlet>
		<portlet-name>edison-template</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
		<css-class-wrapper>edison-template container</css-class-wrapper>
	</portlet>
	
	<portlet>
		<portlet-name>edison-workbench-template</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<css-class-wrapper>edison-workbench-template container-fluid</css-class-wrapper>
	</portlet>	
	
	<portlet>
		<portlet-name>edison-organization-manager</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
	</portlet>		
	
	
	
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
