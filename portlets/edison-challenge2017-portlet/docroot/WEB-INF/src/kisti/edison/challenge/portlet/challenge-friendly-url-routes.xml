<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
   <route>
        <pattern>/add_challenge</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/challengeadmin/edit_challenge.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{field}/add_childchallenge</pattern>
        <implicit-parameter name="mvcPath">/html/challengeadmin/edit_childchallenge.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{field}/{number}/view_childchallenge</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/challengeadmin/view_childchallenge.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{field}/view</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/challengeadmin/view.jsp</implicit-parameter>
    </route>
</routes>