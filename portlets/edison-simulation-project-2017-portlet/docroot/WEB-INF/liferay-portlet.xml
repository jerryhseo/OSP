<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>edison-simulation-project</portlet-name>
		<icon>/icon.png</icon>		
		<css-class-wrapper>edison-simulation-project</css-class-wrapper>
		<add-default-resource>true</add-default-resource>	
	</portlet>
	
	<portlet>
		<portlet-name>edison-use-app</portlet-name>
		<icon>/icon.png</icon>		
		<action-url-redirect>true</action-url-redirect>
		<css-class-wrapper>edison-use-app</css-class-wrapper>	
	</portlet>
	
	<portlet>
		<portlet-name>edison-relate-asset-statistic</portlet-name>
		<icon>/icon.png</icon>		
		<action-url-redirect>true</action-url-redirect>
		<css-class-wrapper>edison-relate-asset-statistic</css-class-wrapper>	
	</portlet>
			
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>