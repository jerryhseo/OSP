<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.kisti.osp.model.FileManagement" />
	<import class="com.kisti.osp.model.SystemProperties" />
	<class name="com.kisti.osp.model.impl.FileManagementImpl" table="OSP_FileManagement">
		<cache usage="read-write" />
		<id name="userId" type="long">
			<generator class="assigned" />
		</id>
		<property name="assigned" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="used" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="lastModified" type="org.hibernate.type.TimestampType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.kisti.osp.model.impl.SystemPropertiesImpl" table="OSP_SystemProperties">
		<cache usage="read-write" />
		<id name="propertyName" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="propertyValue" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="active" type="com.liferay.portal.dao.orm.hibernate.BooleanType" column="active_" />
	</class>
</hibernate-mapping>