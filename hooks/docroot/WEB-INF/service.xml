<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.kisti.osp">
	<author>Jerry h. Seo</author>
	<namespace>OSP</namespace>

    <entity name="FileManagement" local-service="true">
        <column name="userId" type="long" primary="true"></column>
        <column name="assigned" type="int"></column>
        <column name="used" type="int"></column>
        <column name="lastModified" type="Date"></column>
        <column name="status" type="int"></column>
    </entity>
    
    <entity name="SystemProperties" local-service="true">
        <column name="propertyName" type="String" primary="true"></column>
        <column name="propertyValue" type="String"></column>
        <column name="active" type="boolean"></column>
    </entity>
</service-builder>